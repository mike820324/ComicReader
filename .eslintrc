{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-trailing-spaces": 1,
        "prefer-const": 1,
        "max-nested-callbacks": [1, 3],
        "constructor-super": 2,
        "no-this-before-super": 2,
        "react/display-name": 1,
        "react/prop-types": 1,
        "react/wrap-multilines": 2

    }
}
